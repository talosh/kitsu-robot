		<string>192.168.15.99</string>
		<key>DB_USERNAME</key>
		<string>dladmin</string>
		<key>DB_PASSWORD</key>
		<string>mysecretpassword</string>
		<key>SECRET_KEY</key>
		<string>yourrandomsecretkey</string>
		<key>PREVIEW_FOLDER</key>
		<string>/opt/zou/previews</string>
		<key>TMP_DIR</key>
		<string>/opt/zou/tmp</string>
		<key>PATH</key>
		<string>/opt/zou/zouenv/bin:/usr/local/bin:/usr/bin:/bin</string>
