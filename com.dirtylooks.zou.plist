	</dict>
	<key>Label</key>
	<string>com.dirtylooks.zou</string>
	<key>ProgramArguments</key>
	<array>
		<string>/opt/zou/zouenv/bin/gunicorn</string>
		<string>-c</string>
		<string>/etc/zou/gunicorn.conf</string>
		<string>-b</string>
